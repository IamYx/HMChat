{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "module": "es2015",
      "target": "es2015",
      "lib": ["dom", "es6", "es2017"],
      "sourceMap": false,
      "moduleResolution": "node",
      "strict": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false,
      "typeRoots": ["./node_modules/@types"]
    }
  }
  